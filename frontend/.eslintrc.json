{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "prefer-const": "warn",
    "no-console": "warn",
    "react/no-unescaped-entities": "off",
    "@next/next/no-html-link-for-pages": "error",
    "react/display-name": "warn"
  }
} 